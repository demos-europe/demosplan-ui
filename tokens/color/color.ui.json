          "fill": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          }
        },
