{
  "color": {
    "ui": {
      "$description": "Tokens applied to individual UI components and tokens that are rather functional.",
      "cta": {
        "$description": "Used for interactive elements of high importance (eg. actionbox in public area).",
        "dark": {
          "value": "{color.palette.red.dark-2}",
          "themeable": true
        },
        "base": {
          "value": "{color.palette.red.dark-1}",
          "themeable": true
        },
        "light": {
          "value": "{color.palette.red.base}",
          "themeable": true
        },
        "contrast": {
          "$description": "Should pass AAA contrast check with the value of cta-base.",
          "value": "{color.palette.white}",
          "themeable": true
        }
      },
      "header": {
        "$description": "Used to style both the page header and main navigation in public area.",
        "base": {
          "$description": "Default main nav item colors, match fullscreen nav bar background + mobile current item",
          "value": "{color.brand.main.base}",
          "themeable": true
        },
        "base-contrast": {
          "value": "{color.brand.main.contrast}",
          "themeable": true
        },
        "alt": {
          "$description": "Highlighted state for main nav items, matches page header colors.",
          "value": "{color.brand.alt.base}",
          "themeable": true
        },
        "alt-contrast": {
          "value": "{color.brand.alt.contrast}",
          "themeable": true
        },
        "active": {
          "$description": "Hover state for main nav items, matches :active state for mobile item",
          "value": "{color.palette.neutral.light-3}",
          "themeable": true
        },
        "active-contrast": {
          "value": "{color.brand.main.base}",
          "themeable": true
        }
      },
      "nav-alt": {
        "$description": "Style the appearance of branded secondary navigation (project switcher, footer in public area).",
        "bg": {
          "value": "{color.palette.blue.light-3}",
          "themeable": true
        },
        "text": {
          "value": "{color.palette.neutral.dark-1}",
          "themeable": true
        },
        "link": {
          "value": "{color.brand.main.base}",
          "themeable": true
        },
        "link-hover": {
          "value": "{color.brand.highlight.base}",
          "themeable": true
        }
      },
      "interactive": {
        "$description": "Color used to indicate interactive elements.",
        "base": {
          "$description": "Default state.",
          "value": "{color.palette.blue.base}",
          "themeable": true
        },
        "hover": {
          "$description": "Hover state.",
          "value": "{color.palette.blue.dark-1}",
          "themeable": true
        },
        "active": {
          "$description": "Active/pressed state.",
          "value": "{color.palette.blue.dark-2}",
          "themeable": true
        },
        "muted": {
          "$description": "Use for interactive elements that reveal their base state on:hover of a parent element, eg. in a table row. Note that this is not to be conflated with the disabled state, which is communicated via opacity.",
          "value": "{color.palette.neutral.light-1}",
          "themeable": true
        }
      },
      "message": {
        "$description": "Color used to communicate feedback.",
        "info": {
          "$description": "Use these colors to communicate uncritical information.",
          "bg": {
            "value": "{color.palette.blue.light-3}",
            "themeable": true
          },
          "border": {
            "value": "{color.palette.blue.light-1}",
            "themeable": true
          },
          "fill": {
            "value": "{color.palette.blue.dark-1}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.blue.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.blue.dark-2}",
            "themeable": true
          }
        },
        "warning": {
          "$description": "Use these colors to noticeable communicate uncritical information.",
          "bg": {
            "value": "{color.palette.yellow.light-3}",
            "themeable": true
          },
          "border": {
            "value": "{color.palette.yellow.base}",
            "themeable": true
          },
          "fill": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          }
        },
        "severe": {
          "$description": "Use these colors to communicate importance, danger, and immediate action.",
          "bg": {
            "value": "{color.palette.red.light-3}",
            "themeable": true
          },
          "border": {
            "value": "{color.palette.red.light-1}",
            "themeable": true
          },
          "fill": {
            "value": "{color.palette.red.dark-1}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.red.dark-1}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.red.dark-2}",
            "themeable": true
          }
        },
        "success": {
          "$description": "Use these colors to communicate successful state change.",
          "bg": {
            "value": "{color.palette.green.light-3}",
            "themeable": true
          },
          "border": {
            "value": "{color.palette.green.light-1}",
            "themeable": true
          },
          "fill": {
            "value": "{color.palette.green.dark-1}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.green.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.green.dark-2}",
            "themeable": true
          }
        },
        "debug": {
          "$description": "Use these colors to communicate information for debugging purposes.",
          "bg": {
            "value": "{color.palette.purple.light-3}",
            "themeable": true
          },
          "border": {
            "value": "{color.palette.purple.light-1}",
            "themeable": true
          },
          "fill": {
            "value": "{color.palette.purple.dark-1}",
            "themeable": true
          },
          "interactive": {
            "value": "{color.palette.purple.dark-1}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.purple.dark-2}",
            "themeable": true
          }
        }
      },
      "status": {
        "$description": "Color used to communicate state.",
        "progress": {
          "$description": "Use these colors to communicate that something is in progress",
          "fill": {
            "value": "{color.palette.blue.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.blue.dark-1}",
            "themeable": true
          }
        },
        "moved": {
          "$description": "Use these colors to communicate something has moved.",
          "fill": {
            "value": "{color.palette.yellow.dark-1}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.yellow.dark-2}",
            "themeable": true
          }
        },
        "failed": {
          "$description": "Use these colors to communicate that an action has failed on an item.",
          "fill": {
            "value": "{color.palette.red.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.red.dark-1}",
            "themeable": true
          }
        },
        "complete": {
          "$description": "Use these colors to communicate that an action has completed, or something has been added.",
          "fill": {
            "value": "{color.palette.green.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.green.dark-1}",
            "themeable": true
          }
        },
        "neutral": {
          "$description": "Use these colors if no other option applies.",
          "fill": {
            "value": "{color.palette.neutral.base}",
            "themeable": true
          },
          "text": {
            "value": "{color.palette.neutral.dark-1}",
            "themeable": true
          }
        }
      }
    }
  }
}
